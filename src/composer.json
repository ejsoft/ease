{
  "name": "ejsoft/core",
  "type": "project",
  "version": "1.0.0-dev",
  "keywords": [
    "cms",
    "yii2"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-zip": "*",
    "ext-mcrypt": "*",
    "ejsoft/composer": "~1.0.2",
    "yiisoft/yii2": "~2.0.12@stable",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "yiisoft/yii2-smarty": "~2.0.0",
    "yiisoft/yii2-twig": "~2.1.0",
    "yiisoft/yii2-debug": "~2.0.0",
    "symfony/yaml": "*",
    "mobiledetect/mobiledetectlib": "*",
    "giggsey/libphonenumber-for-php": "~8",
    "kdn/yii2-domain-validator": "*",
    "symfony/process": "*",
    "danielstjules/stringy": "*",
    "longman/ip-tools": "*"
  },
  "autoload": {
    "psr-4": {
      "ease\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
